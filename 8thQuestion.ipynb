    "#8. Use NumPy to create a 5x5 identity matrix, then extract its diagonal elements\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "arr = np.eye(5)\n",
    "print(\"Identity matrix:\")\n",
    "print(arr)\n",
    "\n",
    "print(\"Diagonal elements:\")\n",
    "print(np.diag(arr))"
